<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import org.controlsfx.control.PropertySheet?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="net.ncguy.tools.debug.view.EntityController">

    <SplitPane dividerPositions="0.25, 0.75" AnchorPane.topAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.bottomAnchor="0">
        <ScrollPane fitToHeight="true" fitToWidth="true">
            <TreeView fx:id="Container" />
        </ScrollPane>
        <ScrollPane fitToHeight="true" fitToWidth="true">
            <TreeView fx:id="ComponentContainer" />
        </ScrollPane>
        <ScrollPane fitToHeight="true" fitToWidth="true">
            <PropertySheet fx:id="ComponentDetails" />
        </ScrollPane>
    </SplitPane>

</AnchorPane>
